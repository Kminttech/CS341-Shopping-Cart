@model cs341.Models.User

<h1>Register</h1>
<hr />
<div class="row well">
    <div class="col-md-4">
        <div class="form-group">
            <h4>Username</h4>
            <input id="registerUsername" class="form-control" />
        </div>
        <div class="form-group">
            <h4>Password</h4>
            <input id="registerPassword" type="password" class="form-control" />
        </div>
        <div class="form-group">
            <input id="register-submit" type="submit" value="Register" class="btn btn-default" onclick="register()"/>
        </div>
    </div>
</div>
